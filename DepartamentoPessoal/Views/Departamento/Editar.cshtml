@{
    ViewData["Title"] = "Editar campo do departamento Pessoal";
}

<div class="text-center">
    <h2 class="display-4">Editar campo do departamento Pessoal</h2>
</div>

<form>
  <div class="form-group">
    <label for="nome">Nome</label>
    <input type="text" class="form-control"  placeholder="Digite o seu nome">
  </div>
  <div class="form-group">
    <label for="e-mail">E-mail</label>
    <input type="text" class="form-control"  placeholder="Digite o seu email">
  </div>
  <div class="form-group">
    <label for="cpf">Cpf</label>
    <input type="text" class="form-control"  placeholder="Digite o seu cpf">
  </div>
  <div class="form-group">
    <label for="rg">Rg</label>
    <input type="text" class="form-control"  placeholder="Digite o seu rg">
  </div>
  <div class="form-group">
    <label for="rg">data Nascimento</label>
    <input type="text" class="form-control"  placeholder="Digite a sua data de nascimento">
  </div>
  <div class="form-group">
    <label for="nomePai">Nome do Pai</label>
    <input type="text" class="form-control"  placeholder="Digite o nome dpo pai">
  </div>
  <div class="form-group">
    <label for="nomemae">Nome da Mãe</label>
    <input type="text" class="form-control"  placeholder="Digite o nome da Mãe">
  </div>
  <button type="submit" class="btn btn-primary btn-block">Cadastar Dados</button>
</form>
